{
  "name": "github-stars",
  "version": "1.0.0",
  "description": "A script checking stars number at a given point in time for a given GitHub repo",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && babel --copy-files src --out-dir build --ignore tests/",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "test": "mocha -r babel-register --recursive src/tests --timeout 5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oprogramador/github-stars.git"
  },
  "keywords": ["github","stars","star","past","history"],
  "author": "oprogramador",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oprogramador/github-stars/issues"
  },
  "homepage": "https://github.com/oprogramador/github-stars#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.6.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-improved": "^2.3.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "mocha": "^3.5.3",
    "sinon": "^2.4.1",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^2.14.0"
  }
}
